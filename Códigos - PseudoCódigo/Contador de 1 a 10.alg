algoritmo "Contador1a10"
var
   C,numero,passo_2,inicio_1:Inteiro
inicio
      Escreval("Deseja começar a contar a partir de qual número? ")
      Leia(inicio_1)
      Escreval("Até qual número deseja contar? ")
      Leia(numero)
      Escreval("Qual o passo? ")
      Leia(passo_2)
      Para C <- inicio_1 ate numero passo passo_2  faca
           Escreval(C)
      FimPara
fimalgoritmo
