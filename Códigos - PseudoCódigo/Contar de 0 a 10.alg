algoritmo "Conte até onde quiser"
var
   numero_desejado, numero : Inteiro
inicio
      Escreval("Até qual número deseja contar? ")
      Leia(Numero_desejado)
      Escreval()
      numero<-numero_desejado
      Enquanto (numero >= 0) faca
               Escreval(numero)
               numero <- numero - 1
      FimEnquanto
fimalgoritmo
