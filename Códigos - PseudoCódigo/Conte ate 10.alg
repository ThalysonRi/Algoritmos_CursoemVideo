algoritmo "conteate10"
var
   contador,salto, N1 : Inteiro
inicio
      Escreva("Até qual número deseja contar? ")
      Leia(N1)
      Escreva("Qual será o valor do salto? ")
      Leia(salto)
      contador <- 0
      Enquanto (N1 >= contador) faca
               Escreval(contador)
               contador <- contador + salto
      FimEnquanto
      Escreval("Terminei de contar")
fimalgoritmo
