algoritmo "Creuza03"
// Função :
// Autor :
// Data : 02/04/2022
// Seção de Declarações 
var
   Fahrenheit,Celsius : Real

inicio
      Escreval("Qual a temperatura informada?")
      Leia (Fahrenheit)
      Celsius <-   (Fahrenheit - 32) / 1.8
      Escreva ("A temperatura em celsius é de ",Celsius:4:1,"º.")
// Seção de Comandos 
fimalgoritmo
