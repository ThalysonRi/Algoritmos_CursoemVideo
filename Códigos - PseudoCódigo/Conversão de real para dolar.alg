algoritmo "Creuza02"
// Função :
// Autor :
// Data : 02/04/2022
// Seção de Declarações 
var
   Rea1l,Dolar,Conversao:Real

inicio
      Escreva("Quantos reais eu tenho? R$ ")
      Leia(Rea1l)
      Escreval("Qual o valor do dolár?")
      Leia(Dolar)
      Conversao <- (Rea1l/Dolar)
      Escreva("O valor de ","R$",Rea1l, " em dolár é de",Conversao)

// Seção de Comandos 
fimalgoritmo
